{
  "name": "ng-restclient",
  "version": "1.0.8",
  "description": "針對Angular5+提供使用修飾器定義產生的REST Client",
  "main": "index.js",
  "scripts": {
    "clearnode": "rimraf ./node_modules",
    "copy": "copyfiles package.json README.md ./dist",
    "cleanup": "rimraf ./tmp && rimraf ./dist",
    "compile": "ngc -p tsconfig.json",
    "build": "npm run cleanup && npm run compile && npm run copy"
  },
  "keywords": ["REST", "Angular"],
  "author": "xupeiyao",
  "license": "MIT",
  "homepage": "https://github.com/XuPeiYao/ngRestClient",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/XuPeiYao/ngRestClient.git"
  },
  "dependencies": {
    "@angular/core": "^5.0.0",
    "@angular/http": "^5.0.0",
    "rxjs": "^5.5.2"
  },
  "devDependencies": {
    "@angular/compiler": "^5.0.0",
    "@angular/cli": "1.5.0",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/language-service": "^5.0.0",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.2.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.4.2",
    "rimraf": "^2.5.4",
    "copyfiles": "^1.0.0"
  }
}
